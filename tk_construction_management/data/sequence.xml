<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_construction_site" model="ir.sequence">
            <field name="name">Construction Sequence</field>
            <field name="code">construction.details</field>
            <field name="prefix">CS/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_job_costing" model="ir.sequence">
            <field name="name">Job Costing Sequence</field>
            <field name="code">job.costing</field>
            <field name="prefix">JS/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_construction_scrap_order" model="ir.sequence">
            <field name="name">Scrap Order Sequence</field>
            <field name="code">scrap.order</field>
            <field name="prefix">SCRAP/CS/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>